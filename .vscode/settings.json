{
    "typescript.disableAutomaticTypeAcquisition": true,
    "extensions.showRecommendationsOnlyOnDemand": true,
    "git.autofetch": true,
    "files.autoSave": "onWindowChange",
    "editor.defaultFormatter": "vscode.configuration-editing",
    "editor.renderWhitespace": "all",
    "java.codeGeneration.hashCodeEquals.useJava7Objects": true,
    "java.codeGeneration.generateComments": true,
    "java.codeGeneration.hashCodeEquals.useInstanceof": true,
    "java.codeGeneration.toString.codeStyle": "STRING_FORMAT",
    "java.codeGeneration.toString.limitElements": 1,
    "java.implementationsCodeLens.enabled": true,
    "java.maven.downloadSources": true,
    "java.saveActions.organizeImports": true,
    "java.referencesCodeLens.enabled": true,
    "java.signatureHelp.enabled": true,
    "java.debug.logLevel": "verbose",
    "java.debug.settings.maxStringLength": 1,
    "java.debug.settings.showHex": true,
    "java.debug.settings.showStaticVariables": true,
    "java.debug.settings.showQualifiedNames": true
}